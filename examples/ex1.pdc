rule unseq1 (A, B) {
    unseq {
        A -> B: S1
        A -> B: S2
        A -> B: S3
    }
}

rule unseq2 (A, B) {
    unseq {
        seq {
            A -> B: S1
            A -> B: S2
        }
        seq {
            A -> B: S3
            A -> B: S4
        }
    }
}

