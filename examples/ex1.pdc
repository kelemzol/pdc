module example

export good-rule
export good-rule2
export bad-rule
export simple-rule2

rule good-rule(A, B) {
    A -> B : REQ
    B -> A : WHUT
}

rule good-rule2(A, B) {
    A -> B : REQ
    B -> A : WHUT
    B -> A : WHUT
    B -> A : WAWA
    B -> A : WHUT
    B -> A : RES
}

rule bad-rule(A, B) {
    A -> B : RES
}

rule simple-rule(A, B) {
	A -> B : DD
}

rule simple-rule2(A, B) {
    A -> B : REQ
    many-of {
        B -> A : WHUT
        B -> A : WAWA
    }
    B -> A : RES
}
