rule opt1(A,B) {
    optional A -> B: MSG
}

rule opt2(A,B) {
    optional A -> B: MSG
    A -> B: END
}

rule opt3(A,B) {
    optional seq {
        A -> B: MSG1
        A -> B: MSG2
    }
    optional seq {
        A -> B: MSG3
        A -> B: MSG4
    }
}

rule opt4(A,B) {
    optional seq {
        A -> B: MSG1
        A -> B: MSG2
    }
    optional seq {
        A -> B: MSG3
        A -> B: MSG4
    }
    A -> B: END
}

rule opt5(A,B) {
    optional one-of {
        A -> B: MSG1
        seq {
            A -> B: MSG2
            A -> B: MSG3
            optional A -> B: MSG4
            A -> B: END2
        }
        optional seq {
            A -> B: MSG5
        }
    }
    A -> B: END
}


